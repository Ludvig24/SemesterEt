<Window x:Class="FørsteSemester.CreateClass"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="CreateClass" Height="700" Width="450">
    <Grid>
        <ComboBox HorizontalAlignment="Center" Margin="0,226,0,0" VerticalAlignment="Top" Width="120" x:Name="AktivitetBox" Text="" Height="20">
            <ComboBoxItem x:Name="Yoga" Content="Yoga"/>
            <ComboBoxItem x:Name="Pilates" Content="Pilates"/>
        </ComboBox>
        <Label Content="Hold navn" HorizontalAlignment="Left" Margin="181,366,0,0" VerticalAlignment="Top" Width="84"/>
        <TextBox x:Name="HoldNavnBox" HorizontalAlignment="Center" Margin="0,251,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PladsBox" HorizontalAlignment="Center" Margin="0,274,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AlderMaxBox" HorizontalAlignment="Center" Margin="0,320,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AlderMinBox" HorizontalAlignment="Center" Margin="0,343,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="OpretHold" Content="Button" HorizontalAlignment="Left" Margin="203,397,0,0" VerticalAlignment="Top" Click="OpretHold_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="163,297,0,0" VerticalAlignment="Top" Width="120" x:Name="KønBox" Text="" Height="20">
            <ComboBoxItem x:Name="M" Content="Mand"/>
            <ComboBoxItem x:Name="F" Content="Kvinde"/>
            <ComboBoxItem x:Name="B" Content="Begge"/>
        </ComboBox>

    </Grid>
</Window>

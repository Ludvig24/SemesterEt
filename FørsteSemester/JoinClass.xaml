<Window x:Class="FørsteSemester.JoinClass"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="JoinClass" Height="700" Width="450">
    <Grid>
        <Button x:Name="Tilmeld" Content="Tilmeld" HorizontalAlignment="Center" Margin="0,550,0,0" VerticalAlignment="Top" Click="Tilmeld_Click" Height="38" Width="78"/>
        <ListBox Name="ClassesList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="86,109,86,169" SelectionChanged="ListBox_SelectionChanged" RenderTransformOrigin="0.53,0.526"/>
        <Button Content="Tilbage" Name="Tilbageknap" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="46" Width="70" Click="Tilbageknap_Click"/>
        <TextBox HorizontalAlignment="Center" Name="FejlBox" Margin="0,515,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="160" Height="35" Foreground="Black" IsReadOnly="True" BorderBrush="Black" Visibility="Visible"/>

    </Grid>
</Window>

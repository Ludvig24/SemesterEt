<Window x:Class="FørsteSemester.MainWindow"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="450">
    <Grid>
        <Button x:Name="Loggin" Content="Loggin" HorizontalAlignment="Center" Margin="0,357,0,0" VerticalAlignment="Top" Height="48" Width="102" Click="Loggin_Click"/>
        <Button Content="Opret Bruger" HorizontalAlignment="Left" Margin="157,508,0,0" VerticalAlignment="Top" Height="59" Width="122" Click="Button_Click" RenderTransformOrigin="-0.469,0.596"/>
        <TextBox x:Name="BrugernavnBox" HorizontalAlignment="Left" Margin="88,221,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PasswordBox" HorizontalAlignment="Left" Margin="88,264,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Brugernavn" HorizontalAlignment="Left" Margin="8,216,0,0" VerticalAlignment="Top" Height="28" Width="75"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="16,259,0,0" VerticalAlignment="Top" Height="28" Width="60"/>
        <Label Content="Velkommen" Foreground="Blue" HorizontalAlignment="Left" Margin="133,58,0,0" VerticalAlignment="Top" Height="74" Width="200" RenderTransformOrigin="0.5,0.5" FontSize="36">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.063"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>

<Window x:Class="FørsteSemester.MainWindow"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="450">
    <Grid>
        <Button x:Name="Loggin" Content="Login" HorizontalAlignment="Center" Margin="0,342,0,0" VerticalAlignment="Top" Height="48" Width="102" Click="Loggin_Click" FontSize="14"/>
        <Button Content="Opret Bruger" HorizontalAlignment="Center" Margin="0,436,0,0" VerticalAlignment="Top" Height="48" Width="102" Click="Button_Click" RenderTransformOrigin="-0.469,0.596" FontSize="14"/>
        <TextBox x:Name="BrugernavnBox" HorizontalAlignment="Center" Margin="0,226,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PasswordBox" HorizontalAlignment="Center" Margin="0,289,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="BrugerNavn" Content="Brugernavn" HorizontalAlignment="Left" Margin="159,197,0,0" VerticalAlignment="Top" Height="38" Width="92" FontSize="16"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="159,263,0,0" VerticalAlignment="Top" Height="28" Width="81" FontSize="14"/>
        <Label Content="Velkommen" Foreground="Blue" HorizontalAlignment="Left" Margin="112,64,0,0" VerticalAlignment="Top" Height="74" Width="257" RenderTransformOrigin="0.5,0.5" FontSize="48">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.063"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>

<Window x:Class="FørsteSemester.CreateMember"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="CreateMember" Height="700" Width="450">

    <Grid Margin="-1,0,1,0">
        <TextBox HorizontalAlignment="Center" Margin="0,55,0,0" TextWrapping="Wrap" Text="Ny bruger" VerticalAlignment="Top" Width="226" Height="71" FontSize="48" IsReadOnly="True"/>
        <TextBox HorizontalAlignment="Left" Margin="218,226,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="FornavnBox"/>
        <TextBox HorizontalAlignment="Left" Margin="218,278,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="EfternavnBox"/>
        <TextBox HorizontalAlignment="Left" Margin="218,374,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="AlderBox"/>
        <TextBox HorizontalAlignment="Left" Margin="218,417,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="ByBox"/>
        <TextBox HorizontalAlignment="Left" Margin="218,459,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="BrugernavnBox"/>
        <TextBox HorizontalAlignment="Left" Margin="218,500,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="PasswordBox"/>
        <Label Content="Fornavn" HorizontalAlignment="Left" Margin="70,222,0,0" VerticalAlignment="Top"/>
        <Label Content="Efternavn" HorizontalAlignment="Left" Margin="70,274,0,0" VerticalAlignment="Top"/>
        <Label Content="Køn" HorizontalAlignment="Left" Margin="70,320,0,0" VerticalAlignment="Top"/>
        <Label Content="Alder" HorizontalAlignment="Left" Margin="70,370,0,0" VerticalAlignment="Top"/>
        <Label Content="By" HorizontalAlignment="Left" Margin="74,413,0,0" VerticalAlignment="Top"/>
        <Label Content="Brugernavn" HorizontalAlignment="Left" Margin="70,455,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="70,496,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="218,326,0,0" VerticalAlignment="Top" Width="120" Name="KønBox" Text="Vælg køn">
            <ComboBoxItem Name="MandBox" Content="Mand"></ComboBoxItem>
            <ComboBoxItem Name="KvindeBox" Content="Kvinde"></ComboBoxItem>
        </ComboBox>
        <Button x:Name="CreateUser" Content="Opret bruger" HorizontalAlignment="Center" Height="34" Margin="0,597,0,0" VerticalAlignment="Top" Width="182" Click="CreateUser_Click"/>
        <Button x:Name="TilbageKnap" Content="Tilbage" HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="70" Click="Tilbage_Click"/>
        <TextBox Name="Fejlbox" Foreground="Red" HorizontalAlignment="Left" Margin="143,588,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Visibility="Hidden"/>




    </Grid>
</Window>

<Window x:Class="FørsteSemester.CreateMember"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="CreateMember" Height="700" Width="450">

    <Grid Margin="-1,0,1,0">
        <TextBox HorizontalAlignment="Left" Margin="221,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="FornavnBox" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="221,224,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="EfternavnBox" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="221,344,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="AlderBox" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="221,404,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="ByBox" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="221,464,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="BrugernavnBox" Height="20"/>
        <TextBox HorizontalAlignment="Left" Margin="221,524,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Name="PasswordBox" Height="20"/>
        <Label Content="Fornavn" HorizontalAlignment="Left" Margin="95,162,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Efternavn" HorizontalAlignment="Left" Margin="95,222,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Køn" HorizontalAlignment="Left" Margin="95,282,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Alder" HorizontalAlignment="Left" Margin="96,342,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="By" HorizontalAlignment="Left" Margin="96,403,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Brugernavn" HorizontalAlignment="Left" Margin="96,462,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="96,522,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ComboBox HorizontalAlignment="Left" Margin="221,284,0,0" VerticalAlignment="Top" Width="120" Name="KønBox" Text="Vælg køn" Height="20">
            <ComboBoxItem Name="MandBox" Content="Mand"></ComboBoxItem>
            <ComboBoxItem Name="KvindeBox" Content="Kvinde"></ComboBoxItem>
        </ComboBox>
        <Button x:Name="CreateUser" Content="Opret bruger" HorizontalAlignment="Center" Height="34" Margin="0,597,0,0" VerticalAlignment="Top" Width="182" Click="CreateUser_Click"/>
        <Button x:Name="TilbageKnap" Content="Tilbage" HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="70" Click="Tilbage_Click"/>
        <TextBox Name="Fejlbox" Foreground="Red" HorizontalAlignment="Left" Margin="158,537,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Visibility="Hidden"/>
        <Label Content="Ny bruger" HorizontalAlignment="Center" Height="80" Margin="0,48,0,0" VerticalAlignment="Top" Width="234" FontSize="48"/>




    </Grid>
</Window>

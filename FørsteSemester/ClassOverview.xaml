<Window x:Class="FørsteSemester.ClassOverview"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="ClassOverview" Height="700" Width="450">
    <Grid>
        <ListBox Name="ClassesListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="86,109,86,169"/>
        <Button Content="Tilbage" Name="TilbageKnap" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="46" Width="70" Click="TilbageKnap_Click"/>
        <Label Content="Her kan du se&#xD;&#xA;dine tilmeldte hold" HorizontalAlignment="Center" Margin="0,38,0,0" VerticalAlignment="Top" Height="53" Width="163"/>
    </Grid>
</Window>

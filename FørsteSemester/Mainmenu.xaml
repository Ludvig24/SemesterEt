<Window x:Class="FørsteSemester.Mainmenu"
        WindowStartupLocation="CenterScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FørsteSemester"
        mc:Ignorable="d"
        Title="Mainmenu" Height="700" Width="450">
    <Grid>
        <Button x:Name="Logud" Content="Logud" HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="70" Click="Logud_Click"/>
        <Button x:Name="OpretHold" Content="Tilmeld Hold" HorizontalAlignment="Center" Margin="0,177,0,0" VerticalAlignment="Top" Click="Tilmeld_Click"/>
        <Button x:Name="MineHold" Content="Mine Hold" HorizontalAlignment="Center" Margin="0,222,0,0" VerticalAlignment="Top" Height="20" Width="71" Click="MineHold_Click"/>
    </Grid>
</Window>
